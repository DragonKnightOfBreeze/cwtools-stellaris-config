types = {
	## prefix = agenda_
	type[agenda] = {
		path = "game/common/agendas"
		file_extension = .txt
		localisation = {
			## required
			Name = "$"
		}
	}
}

agenda = {
	## replace_scope = { this = leader root = leader from = country }
